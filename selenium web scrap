titlelist=[]
colist=[]
timelist=[]
adresslist=[]
linklist=[]
from selenium import webdriver
driver = webdriver.Chrome()
for pagenum in range(1,4):
    url='https://wuzzuf.net/search/jobs/?a=hpb&q=sales%20manager&start={pagenum}'.format(pagenum=pagenum)
    resp = driver.get(url)
    title = driver.find_elements("xpath",'//h2[@class="css-m604qf"]')
    for i in title:
        titlelist.append(i.text)


    coname = driver.find_elements('xpath','//a[@class="css-17s97q8"]')
    for c in coname :
        colist.append(c.text)

    time = driver.find_elements('xpath','//span[@class="css-1ve4b75 eoyjyou0"]')
    for t in time : 
        timelist.append(t.text)


    adress = driver.find_elements('xpath','//span[@class="css-5wys0k"]')
    for d in adress:
        adresslist.append(d.text)

    links = driver.find_elements("xpath",'//h2//a[@class="css-o171kl"]')
    for l in links :
        linklist.append(l.get_attribute("href"))

    

    

    











driver.quit()


print(titlelist)
print(colist)
print(timelist)
print(adresslist)
print(linklist)

